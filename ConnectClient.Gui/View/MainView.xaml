                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />
                                    </Grid.RowDefinitions>

                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="150" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <TextBlock Style="{StaticResource HeadingTextBlockStyle}"
                                            Grid.ColumnSpan="2">Allgemeine Einstellungen</TextBlock>

                                    <TextBlock Grid.Row="1" Grid.Column="0">Attribut für eindeutige ID</TextBlock>
                                    <TextBox Grid.Row="1" Grid.Column="1" Text="{Binding UniqueIdAttributeName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="2" Grid.Column="0">Art des Benutzernamens</TextBlock>
                                    <ComboBox Grid.Row="2" Grid.Column="1"
                                              ItemsSource="{Binding LdapUsernameProperties}"
                                              SelectedItem="{Binding LdapUsernameProperty, Mode=TwoWay}"></ComboBox>

                                    <TextBlock Style="{StaticResource HeadingTextBlockStyle}"
                                               Grid.Row="3"
                                               Grid.ColumnSpan="2">Server-Einstellungen</TextBlock>

                                    <TextBlock Grid.Row="4" Grid.Column="0">URL</TextBlock>
                                    <TextBox Grid.Row="4" Grid.Column="1" Text="{Binding EndpointUrl, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="5" Grid.Column="0">Token</TextBlock>
                                    <TextBox Grid.Row="5" Grid.Column="1" Text="{Binding EndpointToken, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Style="{StaticResource HeadingTextBlockStyle}"
                                               Grid.Row="6"
                                               Grid.ColumnSpan="2">LDAP-Einstellungen</TextBlock>

                                    <TextBlock Grid.Row="7" Grid.Column="0">Server</TextBlock>
                                    <TextBox Grid.Row="7" Grid.Column="1" Text="{Binding LdapServer, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="8" Grid.Column="0">Port</TextBlock>
                                    <xceed:IntegerUpDown Grid.Row="8" Grid.Column="1"
                                                         Value="{Binding LdapPort, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                         Minimum="0"></xceed:IntegerUpDown>

                                    <CheckBox IsChecked="{Binding LdapUseSsl, Mode=TwoWay}"
                                              Grid.Row="9" Grid.ColumnSpan="2">SSL verwenden</CheckBox>
                                    <CheckBox IsChecked="{Binding LdapUseTls, Mode=TwoWay}"
                                              Grid.Row="10" Grid.ColumnSpan="2">TLS verwenden</CheckBox>

                                    <TextBlock Grid.Row="11" Grid.Column="0">FQDN</TextBlock>
                                    <TextBox Grid.Row="11" Grid.Column="1" Text="{Binding LdapFqdn, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="12" Grid.Column="0">NetBIOS-Name</TextBlock>
                                    <TextBox Grid.Row="12" Grid.Column="1" Text="{Binding LdapNetBIOS, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="13" Grid.Column="0">Benutzername</TextBlock>
                                    <TextBox Grid.Row="13" Grid.Column="1" Text="{Binding LdapUsername, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="14" Grid.Column="0">Password</TextBlock>
                                    <TextBox Grid.Row="14" Grid.Column="1" Text="{Binding LdapPassword, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="15" Grid.Column="0">Fingerabdruck des Zertifikats</TextBlock>
                                    <TextBox Grid.Row="15" Grid.Column="1" Text="{Binding LdapCertificateThumbprint, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />

                                    <TextBlock Grid.Row="16" Grid.Column="0"
                                               VerticalAlignment="Top">Organisationseinheiten</TextBlock>
                                    <ListBox Grid.Row="16" Grid.Column="1"
                                             ItemsSource="{Binding OrganizatioalUnits}"
                                             MaxHeight="150"
                                             SelectionMode="Multiple">
                                        <b:Interaction.Behaviors>
                                            <behavior:SelectedItemsBehavior SelectedItems="{Binding SelectedOrganizationalUnits}"></behavior:SelectedItemsBehavior>
                                        </b:Interaction.Behaviors>
                                    </ListBox>

                                    <Grid Grid.Row="17" Grid.ColumnSpan="2">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                            <ColumnDefinition Width="Auto"></ColumnDefinition>
                                        </Grid.ColumnDefinitions>

